			<div class="row mb-lg-80">
				<div class="col-lg-8 col-sm-12 mb-lg-30">
					{{ component('icon', {
						iconName: 'slata',
						className: "krasny-and-slata__icon",
						attributes: "width=\"108\" height=\"27\""
					})}}
					{{ component('picture', {
						path: "section-2",
						name: strategicReport["krasny-and-slata"].img,
						ext: "jpg",
						width: '728',
						height: "190",
						className: "krasny-and-slata__img",
						lang: lang
					})}}
					<p class="h4 pb-lg-14 mb-lg-22 _line-b _ls-minus-02">
						{{ strategicReport['krasny-and-slata'].subtitle1[lang] | safe }}
					</p>
					<div class="krasny-and-slata__texts">
						<div class="krasny-and-slata__text-wrapper">
							{{ strategicReport['krasny-and-slata'].text1[lang] | safe }}
