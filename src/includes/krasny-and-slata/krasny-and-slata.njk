<section class="krasny-and-slata" id="krasny-yar-and-slata">
		<div class="container">
			<h2 class="h1 pt-lg-32 pt-sm-16 mb-lg-38 mb-sm-32 _line-t _ls-minus-03">
				{{ strategicReport['krasny-and-slata'].title[lang] | safe }}
			</h2>
			<div class="row mb-lg-30">
				<div class="col-lg-6 col-sm-12 mb-lg-30">
					{{ strategicReport['krasny-and-slata'].text[lang] | safe }}
				</div>
				<div class="col-lg-6 col-sm-12">
					<ul class="krasny-and-slata__list list">
						{% for text in strategicReport['krasny-and-slata'].list %}
							<li class="text _fs-14 c-gray-menu-text _mt-10">{{ text[lang] | safe }}</li>
						{% endfor %}
					</ul>
				</div>
			</div>
			<h2 class="h3 pb-lg-10 mb-lg-26 mb-sm-32 _line-b _ls-minus-02">
