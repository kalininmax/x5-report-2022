								</ul>
							</td>
						</tr>
						<tr>
							<td>{{ x5.table.row2.col1[lang] | safe }}</td>
							<td>
								<ul class="list">
									{% for item in x5.table.row2.col2 %}
										<li>{{ item[lang] | safe }}</li>
