						</tr>
					</tbody>
				</table>
			</div>
		</div>
		{{ component('picture', {
				path: "section-4",
				name: x5.img,
				ext: "jpg",
				className: 'mb-lg-80 mb-sm-60 employees__image',
				width: '1112',
				height: "570",
				lang: lang
