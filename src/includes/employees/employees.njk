				<div class="strategic-goals__header">
					<div class="strategic-goals__titles">
						<div class="strategic-goals__title _first">
							<h5 >
								<span class="_col-gr">{{ strategic["title1-left"][lang] | safe }}</span>
								<span class="_col-wh">{{ strategic["title2-left"][lang] | safe }}</span>
							</h5>
						</div>
						<div class="strategic-goals__title _second _right-120 _desktop">
							<h5>
								<span class="_col-gr">{{ strategic["title1-right"][lang] | safe }}</span>
								<span class="_col-wh">{{ strategic["title2-right"][lang] | safe }}</span>
							</h5>
						</div>
					</div>
					{{ component('icon', {
						iconName: strategic.icon,
						className: 'strategic-goals__icon',
						attributes: 'width="60" height="60" viewBox="0 0 36 36"'
